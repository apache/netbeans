trait PropertyHooksTrait {
    // invalid properties
    public $invalidEmptyHook {} // error but parser allows
    private $invalidPrivateFinal {
        final get {} // invalid1
    }
    private $invalidPublic01 {
        public get{} // invalid2
    }
    public $invalidStatic01 {
        static get {} // invalid3
    }
    public static $invalidStatic02 { // invalid4
        get{}
        set{}
    }
    public $invalidGetParam {
        get() { // error but parser allows
            var_dump($value);
        }
    }
    public readonly int $invalidReadonly { // invalid5
        get{}
        set{}
    }
    abstract $invalidPrivateAbstract { // invalid6
        get;
    }
    public abstract $invalidAbstract {
        get {} // invalid7
    }
    public $invalidNonAbstract {
        get; // invalid8
    }
    public $invalidSetRef {
        set(&$value) {} // error but parser allows
    }
    public $invalidVariadic {
        set(...$value) {} // error but parser allows
    }
    public $invalidUnknownHook {
        unknown {} // error
    }
}
