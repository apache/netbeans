    ;

propertyDeclaration
    :
      {isCssPreprocessorSource() && !tokenNameStartsWith("--")}? STAR? property ws? COLON ws? cp_propertyValue //cp_expression may contain the IMPORT_SYM
    | {tokenNameStartsWith("--")}? property ws? COLON ws? componentValueOuter?
