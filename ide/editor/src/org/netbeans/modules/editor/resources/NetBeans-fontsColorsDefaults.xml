<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">

<fontscolors>
    <!-- #146541, do not set font size. By default it has to pick up value of
         the "fontsize" command line option. -->
    <fontcolor name="default" foreColor="black" bgColor="white"/>
    <fontcolor name="char" foreColor="ffCE7B00"/>
    <fontcolor name="comment" foreColor="ff969696"/>
    <fontcolor name="error" waveUnderlined="red"/>
    <fontcolor name="identifier"/>
    <fontcolor name="keyword" foreColor="ff0000E6"/>
    <fontcolor name="number" foreColor="black"/>
    <fontcolor name="operator"/>
    <fontcolor name="string" foreColor="ffCE7B00"/>
    <fontcolor name="separator"/>
    <fontcolor name="warning" waveUnderlined="yellow"/>
    <fontcolor name="whitespace" foreColor="black"/>
    <fontcolor name="mark-occurrences" bgColor="ECEBA3"/>
    
    <!-- OO languages -->    
    <fontcolor name="field" foreColor="ff009900" default="identifier"/>
    <fontcolor name="method" default="identifier"/>
    
    <!-- Tag based languages -->
    
    <fontcolor name="markup-element" default="keyword"/>
    <fontcolor name="markup-attribute" default="field"/>
    <fontcolor name="markup-attribute-value" default="string"/>
    <fontcolor name="entity-reference" foreColor="black" default="default">
        <font style="bold"/>
    </fontcolor>
    
</fontscolors>
