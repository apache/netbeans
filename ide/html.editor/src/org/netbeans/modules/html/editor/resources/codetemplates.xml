<!DOCTYPE codetemplates PUBLIC "-//NetBeans//DTD Editor Code Templates settings 1.0//EN" "http://www.netbeans.org/dtds/EditorCodeTemplates-1_0.dtd">
<codetemplates>
    <codetemplate abbreviation="a" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<a href="${url}">${cursor}</a>]]></code>
    </codetemplate>
    <codetemplate abbreviation="a:target" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<a href="${url}" target="${target}">${cursor}</a>]]></code>
    </codetemplate>
    <codetemplate abbreviation="a:blank" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<a href="${url}" target="_blank">${cursor}</a>]]></code>
    </codetemplate>
    <codetemplate abbreviation="scr" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<script src="${link}">${cursor}</script>]]></code>
    </codetemplate>
    <codetemplate abbreviation="d" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<div>${content}${cursor}</div>]]></code>
    </codetemplate>
    <codetemplate abbreviation="p" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<p>${text}${cursor}</p>]]></code>
    </codetemplate>
