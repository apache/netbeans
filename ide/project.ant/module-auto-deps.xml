<!DOCTYPE transformations PUBLIC "-//NetBeans//DTD Module Automatic Dependencies 1.0//EN" "http://www.netbeans.org/dtds/module-auto-deps-1_0.dtd">
<transformations version="1.0">
    <transformationgroup>
        <description>Major release version of module changed from 0 to 1 to signal stability; update your dependencies</description>
        <transformation>
            <trigger-dependency type="older">
                <module-dependency codenamebase="org.netbeans.modules.project.ant" major="1"/>
            </trigger-dependency>
            <implies>
                <result>
                    <module-dependency codenamebase="org.netbeans.modules.project.ant" major="1" spec="1.3"/>
                </result>
            </implies>
        </transformation>
    </transformationgroup>
    <transformationgroup>
        <description>Separate project.ant into desktop and NetBeans independent and dependent parts</description>
        <transformation>
            <trigger-dependency type="older">
                <module-dependency codenamebase="org.netbeans.modules.project.ant" major="1" spec="1.62"/>
            </trigger-dependency>
            <implies>
                <result>
                    <module-dependency codenamebase="org.netbeans.modules.project.ant.ui" major="1" spec="1.62"/>
                    <module-dependency codenamebase="org.netbeans.modules.project.ant.compat8" major="1" spec="1.62"/>
                </result>
            </implies>
        </transformation>
    </transformationgroup>
</transformations>
