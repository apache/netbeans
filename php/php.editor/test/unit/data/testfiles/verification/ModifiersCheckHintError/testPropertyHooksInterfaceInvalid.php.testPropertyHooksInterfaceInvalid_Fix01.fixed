interface PropertyHookInterface {
    // invalid properties
    public $invalid01 { get; set; } // invalid1
    protected $invalid02 {get; set;} // invalid2
    private $invalid03 { // invalid3
        get;
        set;
    }
    public $invalid04 {
        final get; // invalid4
    }
    final public $invalid05 { // invalid5
        get;
        set;
    }
    public $invalid06 {
        get {} // invalid6
    }
    public $invalid07 {
        set {} // invalid7
    }
    public $invalid08 {
        get {} // invalid8
        set {} // invalid9
    }
    public private(set) int $invalid09 { // invalid10
        set;
    }
    final public int $invalid10 { // invalid11
        set;
    }
    public readonly int $invalid11 { // invalid12
        get;
    }
}
