        <code><![CDATA[<html>
    <head>
        <title>${title}</title>
    </head>
    <body>
        ${cursor}
    </body>
</html>
]]></code>
    </codetemplate>
    <codetemplate abbreviation="ol" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<ol>
    <li>${first}</li>
    <li>${second}</li>
    <li>${third}</li>
    ${cursor}
</ol>
]]></code>
    </codetemplate>
    <codetemplate abbreviation="li" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<li>${item}</li>${cursor}]]></code>
