<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE glassfish-ejb-jar PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 EJB 3.1//EN" "http://raw.githubusercontent.com/eclipse-ee4j/jakartaee-tck/master/lib/dtds/glassfish-ejb-jar_3_1-1.dtd">
<glassfish-ejb-jar>
  <enterprise-beans>
    <ejb>
      <ejb-name>TestingSessionBean</ejb-name>
      <jndi-name>ejb/TestingSessionBean</jndi-name>
      <resource-ref>
        <res-ref-name>myTestingDatabase</res-ref-name>
        <jndi-name>myTestingDatabase</jndi-name>
      </resource-ref>
      <resource-ref>
        <res-ref-name>jms/TestingMessageBeanFactory</res-ref-name>
        <jndi-name>jms/TestingMessageBeanFactory</jndi-name>
      </resource-ref>
      <message-destination-ref>
        <message-destination-ref-name>jms/TestingMessageBean</message-destination-ref-name>
        <jndi-name>jms/TestingMessageBean</jndi-name>
      </message-destination-ref>
    </ejb>
    <ejb>
      <ejb-name>TestingEntityBean</ejb-name>
      <jndi-name>ejb/TestingEntityBean</jndi-name>
    </ejb>
    <ejb>
      <ejb-name>TestingMessageBean</ejb-name>
      <jndi-name>jms/TestingMessageBean</jndi-name>
      <mdb-connection-factory>
        <jndi-name>jms/TestingMessageBeanFactory</jndi-name>
      </mdb-connection-factory>
    </ejb>
  </enterprise-beans>
</glassfish-ejb-jar>
