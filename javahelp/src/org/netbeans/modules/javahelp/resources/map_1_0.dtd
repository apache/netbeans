<!-- A Map.  It is versioned -->

<!ELEMENT map (mapID*)>
  <!ATTLIST map version CDATA #FIXED "1.0">
  <!ATTLIST map xml:lang CDATA #IMPLIED>

<!-- an map ID pair -->

<!ELEMENT mapID EMPTY>
  <!ATTLIST mapID xml:lang CDATA #IMPLIED>
  <!ATTLIST mapID target CDATA #REQUIRED>
  <!ATTLIST mapID url CDATA #REQUIRED>

