<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Menu">
        <folder name="GoTo">
            <file name="org-netbeans-core-actions-JumpNextAction.shadow">
                <attr name="originalFile" stringvalue="Actions/System/org-netbeans-core-actions-JumpNextAction.instance"/>
                <attr name="position" intvalue="2300"/>
            </file>
            <file name="org-netbeans-core-actions-JumpPrevAction.shadow">
                <attr name="originalFile" stringvalue="Actions/System/org-netbeans-core-actions-JumpPrevAction.instance"/>
                <attr name="position" intvalue="2400"/>
            </file>
        </folder>
        <!-- quick search enabled for ide in toolbar by default -->
        <file name="Spacer.instance">
            <attr name="instanceCreate" methodvalue="javax.swing.Box.createHorizontalGlue"/>
            <attr name="position" intvalue="9005"/>
        </file>

        <file name="org-netbeans-modules-quicksearch-QuickSearchAction.shadow">
            <attr name="originalFile" stringvalue="Actions/Edit/org-netbeans-modules-quicksearch-QuickSearchAction.instance"/>
            <attr name="position" intvalue="9010"/>
        </file>

    </folder>
    <folder name="UI">
        <folder name="Runtime">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.ide.resources.Bundle"/>
            <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/core/ide/resources/services.gif"/>
        </folder>
    </folder>
    <folder name="Toolbars">
        <!-- specify toolbar config for ide, overrides configuration from core/ui.
             if you modify this file, please review
             org/netbeans/core/ui/resources/Standard.xml also, and sync
             the changes if appropriate -->
        <file name="Standard.xml" url="Standard.xml">
            <attr name="position" intvalue="800"/>
            <attr name="weight" intvalue="100"/><!-- override core.ui -->
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.ide.resources.Bundle"/>
        </file>
        <file name="Debugging.xml" url="Debugging.xml">
            <attr name="position" intvalue="900"/>
            <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.core.ide.resources.Bundle"/>
        </file>
    </folder>
    <folder name="Windows2">
        <folder name="Groups">
            <folder name="OpenedProjects">
                <file name="services.wstcgrp" url="services.wstcgrp"/>
            </folder>
            <file name="InitialLayout.wsgrp" url="InitialLayout.wsgrp"/>
            <!-- TopComponents in this group are opened when Start Page is closed.-->
            <folder name="InitialLayout">
                <file name="services.wstcgrp" url="services.wstcgrp"/>
            </folder>
        </folder>
    </folder>
</filesystem>
