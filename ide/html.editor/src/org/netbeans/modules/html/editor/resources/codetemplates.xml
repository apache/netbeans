    </codetemplate>
    <codetemplate abbreviation="ul" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<ul>
    <li>${first}</li>
    <li>${second}</li>
    <li>${third}</li>${cursor}
</ul>
]]></code>
    </codetemplate>
    <codetemplate abbreviation="form" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<form action="${action}">${cursor}</form>]]></code>
