    </api>
   </li>
   <li>
    <api group="java" name="OptionsAPI" type="import" category="official">
     <p>
      Miscellaneous internal purposes (not exposed in the GUI).
     </p>
    </api>
   </li>
  </ul>
  <!-- XXX mdr for listening to project main class chg, etc. -->
