<bindings>
    <bind actionName="fast-import" key="OS-I"/>
    <bind actionName="fix-uses" key="DS-I"/>
    <bind actionName="complete-line" key="D-SEMICOLON"/>
    <bind actionName="complete-line-newline" key="DS-SEMICOLON"/>
</bindings>
