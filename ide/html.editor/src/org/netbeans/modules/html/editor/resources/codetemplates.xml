        <code><![CDATA[<main>${cursor}</main>]]></code>
    </codetemplate>
    <codetemplate abbreviation="nav" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<nav>${cursor}</nav>]]></code>
    </codetemplate>
    <codetemplate abbreviation="sec" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<section>${cursor}</section>]]></code>
    </codetemplate>
    <codetemplate abbreviation="h1" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<h1>${title}</h1>${cursor}]]></code>
    </codetemplate>
