        <code><![CDATA[<option value="${value}">${text}</option>${cursor}]]></code>
    </codetemplate>
    <codetemplate abbreviation="ad" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<address>${cursor}</address>]]></code>
    </codetemplate>
    <codetemplate abbreviation="art" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<article class="${class}">${cursor}</article>]]></code>
    </codetemplate>
    <codetemplate abbreviation="as" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<aside>${cursor}</aside>]]></code>
    </codetemplate>
