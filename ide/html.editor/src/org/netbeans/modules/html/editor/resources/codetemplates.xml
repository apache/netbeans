        <code><![CDATA[<input type="${type}" name="${name}"${cursor}>]]></code>
    </codetemplate>
    <codetemplate abbreviation="in:button" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<input type="button" value="${Button}"${cursor}>]]></code>
    </codetemplate>
