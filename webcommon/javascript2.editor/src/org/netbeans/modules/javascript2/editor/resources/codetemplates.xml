<!DOCTYPE codetemplates PUBLIC  "-//NetBeans//DTD Editor Code Templates settings 1.0//EN"
                                "http://www.netbeans.org/dtds/EditorCodeTemplates-1_0.dtd">

<codetemplates>

    <!-- base JavaScript structures -->
    <codetemplate abbreviation="catch" contexts="JavaScript-Code">
        <code>
<![CDATA[catch (${exception}) {
    ${cursor}
}
]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="do" contexts="JavaScript-Code">
        <code>
<![CDATA[do {
    ${cursor}
} while (${expr default="true"});]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="else" contexts="JavaScript-Code">
        <code>
<![CDATA[else {
	${cursor}
}
]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="elseif" contexts="JavaScript-Code">
        <code>
<![CDATA[else if (${condition}) {
	${cursor}
}
]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="fcom" contexts="JavaScript-Code">
        <code>
<![CDATA[${no-format}//<editor-fold defaultstate="collapsed" desc="${comment}">
${selection}${cursor}
//</editor-fold>
]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="fom" contexts="JavaScript-Code">
        <code>
<![CDATA[for (var ${var default="i"} = 0, ${max} = ${limit default="10"}; ${var} < ${max}; ${var}++) {
    ${selection}${cursor}
}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="for" contexts="JavaScript-Code">
        <code>
<![CDATA[for (var ${var default="i"} = 0; ${var} < ${max}; ${var}++) {
    ${selection}${cursor}
}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="forin" contexts="JavaScript-Code">
        <code>
<![CDATA[for (var ${item} in ${object}) {
    ${selection}${cursor}
}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="fun" descriptionId="CT_fun" contexts="JavaScript-Code">
        <code>
<![CDATA[
function ${name}(${parameters}) {
    ${selection}${cursor}
}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="func" contexts="JavaScript-Code">
        <code>
<![CDATA[/**
 * ${1 default="Comment"}
 */
function ${name}() {
    ${cursor}
}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="funcp" contexts="JavaScript-Code">
        <code>
<![CDATA[/**
 * ${1 default="Comment"}
 * @param {${type}} ${parameter}
 */
function ${name}(${parameter}) {
    ${cursor}
}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="function" contexts="JavaScript-Code">
        <code>
<![CDATA[/**
 * ${1 default="Comment"}
 */
function ${name}(${parameters}) {
    ${cursor}
}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="afun" contexts="JavaScript-Code">
        <code>
<![CDATA[function(${cursor}) {
    
}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="afunn" contexts="JavaScript-Code">
        <code>
<![CDATA[function(${cursor}) {
    
};]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="it" contexts="JavaScript-Code">
        <code>
<![CDATA[it('${cursor}', function() {
    
});]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="des" contexts="JavaScript-Code">
        <code>
<![CDATA[describe('${cursor}', function() {
    
});]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="if" descriptionId="CT_if" contexts="JavaScript-Code">
        <code>
<![CDATA[if (${expr}) {
    ${selection}${cursor}
}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="ife" descriptionId="CT_ife" contexts="JavaScript-Code">
        <code>
<![CDATA[if (${expr}) {
   ${selection}${cursor}
} else {
}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="lb" contexts="JavaScript-Code">
        <code>
<![CDATA[${label}:
    ${selection}${cursor}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="new" contexts="JavaScript-Code">
        <code><![CDATA[${no-indent}var ${name} = new ${type}(${arguments});]]></code>
    </codetemplate>
    <codetemplate abbreviation="sw" contexts="JavaScript-Code">
        <code>
<![CDATA[switch (${key}) {
    case ${value}:
        ${cursor}
        break;

    default:
        ${defaultCode default=""}
        break;
}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="the" contexts="JavaScript-Code">
        <code><![CDATA[${no-indent}throw new Error("${errorMessage}");]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="try" xml:space="preserve" contexts="JavaScript-Code">
        <code>
<![CDATA[try {
    ${selection}${cursor}
} catch (${e}) {
    ${catchcode default=""}
}
]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="us" contexts="JavaScript-Code">
        <code><![CDATA[${no-indent}"use strict"]]></code>
    </codetemplate>
    <codetemplate abbreviation="var" contexts="JavaScript-Code">
        <code><![CDATA[${no-indent}var ${name} = ${value};]]></code>
    </codetemplate>
    <codetemplate abbreviation="wh" contexts="JavaScript-Code">
        <code>
<![CDATA[while (${expr default="true"}) {
    ${selection}${cursor}
}]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="wi" contexts="JavaScript-Code">
        <code>
<![CDATA[with (${object}) {
    ${selection}${code default=""}
}]]>
        </code>
    </codetemplate>

    <!-- JavaScript keyword, most used words shortcuts -->
    <codetemplate abbreviation="br" contexts="JavaScript-Code"><code><![CDATA[${no-indent}break;]]></code></codetemplate>
    <codetemplate abbreviation="ca" contexts="JavaScript-Code"><code><![CDATA[${no-indent}case ]]></code></codetemplate>
    <codetemplate abbreviation="ct" contexts="JavaScript-Code"><code><![CDATA[${no-indent}catch]]></code></codetemplate>
    <codetemplate abbreviation="co" contexts="JavaScript-Code"><code><![CDATA[${no-indent}continue]]></code></codetemplate>
    <codetemplate abbreviation="df" contexts="JavaScript-Code"><code><![CDATA[${no-indent}default:]]></code></codetemplate>
    <codetemplate abbreviation="eq" contexts="JavaScript-Code"><code><![CDATA[${no-indent}equals ]]></code></codetemplate>
    <codetemplate abbreviation="fa" contexts="JavaScript-Code"><code><![CDATA[${no-indent}false]]></code></codetemplate>
    <codetemplate abbreviation="fi" contexts="JavaScript-Code"><code><![CDATA[${no-indent}finally]]></code></codetemplate>
    <codetemplate abbreviation="le" contexts="JavaScript-Code"><code><![CDATA[${no-indent}length]]></code></codetemplate>
    <codetemplate abbreviation="iof" contexts="JavaScript-Code"><code><![CDATA[${no-indent}instanceof ]]></code></codetemplate>
    <codetemplate abbreviation="nu" contexts="JavaScript-Code"><code><![CDATA[${no-indent}null]]></code></codetemplate>
    <codetemplate abbreviation="ob" contexts="JavaScript-Code"><code><![CDATA[${no-indent}object]]></code></codetemplate>
    <codetemplate abbreviation="pro" contexts="JavaScript-Code"><code><![CDATA[${no-indent}prototype]]></code></codetemplate>
    <codetemplate abbreviation="re" contexts="JavaScript-Code"><code><![CDATA[${no-indent}return ]]></code></codetemplate>
    <codetemplate abbreviation="th" contexts="JavaScript-Code"><code><![CDATA[${no-indent}throw ]]></code></codetemplate>
    <codetemplate abbreviation="thn" contexts="JavaScript-Code"><code><![CDATA[${no-indent}throw new]]></code></codetemplate>
    <codetemplate abbreviation="tr" contexts="JavaScript-Code"><code><![CDATA[${no-indent}true]]></code></codetemplate>
    <codetemplate abbreviation="ty" contexts="JavaScript-Code"><code><![CDATA[${no-indent}typeof]]></code></codetemplate>
    <codetemplate abbreviation="va" contexts="JavaScript-Code"><code><![CDATA[${no-indent}value]]></code></codetemplate>

    <!-- additional JavaScript code templates -->
    <codetemplate abbreviation="json" contexts="JavaScript-Code">
        <code><![CDATA[{"${field}": "${value}"}]]></code>
    </codetemplate>
    <codetemplate abbreviation="prf" contexts="JavaScript-Code">
        <code>
<![CDATA[${propertyName}: function(${parameters}) {
    ${cursor}
},]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="pro" contexts="JavaScript-Code">
        <code>
<![CDATA[${propertyName}: {
    ${cursor}
},]]>
        </code>
    </codetemplate>
    <codetemplate abbreviation="prt" contexts="JavaScript-Code">
        <code><![CDATA[${propertyName}: ${value},]]></code>
    </codetemplate>
    <codetemplate abbreviation="to" contexts="JavaScript-Code">
        <code><![CDATA[typeof(${object})]]></code>
    </codetemplate>

    <!-- Client-Side and W3C DOM JavaScript code templates -->
    <codetemplate abbreviation="al" contexts="JavaScript-Code">
        <code><![CDATA[window.alert(${message});]]></code>
    </codetemplate>
    <codetemplate abbreviation="cond" contexts="JavaScript-Code">
        <code><![CDATA[window.console.debug(${debug});]]></code>
    </codetemplate>
    <codetemplate abbreviation="coni" contexts="JavaScript-Code">
        <code><![CDATA[window.console.info(${info});]]></code>
    </codetemplate>
    <codetemplate abbreviation="conl" contexts="JavaScript-Code">
        <code><![CDATA[window.console.log(${log});]]></code>
    </codetemplate>
    <codetemplate abbreviation="conw" contexts="JavaScript-Code">
        <code><![CDATA[window.console.warn(${warn});]]></code>
    </codetemplate>
    <codetemplate abbreviation="cl" contexts="JavaScript-Code">
        <code><![CDATA[console.log(${log});]]></code>
    </codetemplate>
    <codetemplate abbreviation="dw" contexts="JavaScript-Code">
        <code><![CDATA[document.write(${message});]]></code>
    </codetemplate>
    <codetemplate abbreviation="slf" contexts="JavaScript-Code">
        <code><![CDATA[var self = this;]]></code>
    </codetemplate>
    <codetemplate abbreviation="do" contexts="JavaScript-Code"><code><![CDATA[${no-indent}document]]></code></codetemplate>
    <codetemplate abbreviation="lo" contexts="JavaScript-Code"><code><![CDATA[${no-indent}window.location]]></code></codetemplate>
    <codetemplate abbreviation="hi" contexts="JavaScript-Code"><code><![CDATA[${no-indent}window.history]]></code></codetemplate>
    <codetemplate abbreviation="win" contexts="JavaScript-Code"><code><![CDATA[${no-indent}window]]></code></codetemplate>
    <codetemplate abbreviation="be" contexts="JavaScript-Code">
        <code><![CDATA[beforeEach(function() {
    ${cursor}
});]]></code>
    </codetemplate>
    <codetemplate abbreviation="ae" contexts="JavaScript-Code">
        <code><![CDATA[afterEach(function() {
    ${cursor}
});]]></code>
    </codetemplate>
</codetemplates>
