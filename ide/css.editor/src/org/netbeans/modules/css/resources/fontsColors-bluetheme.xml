<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">

<fontscolors>
    
    <fontcolor name="at_rule_symbol" foreColor="cyan">
        <font style="bold" />
    </fontcolor>
    
    <!--"semantic" highlighting:-->
    
    <!-- css property name -->
    <fontcolor name="mod-custom1" default="keyword"/> 
    
    <!-- vendor specific property name -->
    <fontcolor name="mod-custom2" default="keyword">
        <font style="italic" />
    </fontcolor>
    
    <!-- media queries: media type (screen, printer, ...) -->
    <fontcolor name="mod-custom3" foreColor="pink">
        <font style="italic" />
    </fontcolor>
    
    <!-- element, class, id selectors -->
    <fontcolor name="mod-method" foreColor="B2CCFF" >
        <!--<font style="bold" />-->
    </fontcolor>
    
    <!-- namespace prefix -->
    <fontcolor name="mod-constructor">
        <!--<font style="bold" />-->
    </fontcolor>

    <!-- pseudo class -->
    <fontcolor name="mod-class" >
        <font style="italic" />
    </fontcolor>
    
    <!-- less/sass variable -->
    <fontcolor name="mod-local-variable" foreColor="009090" >
        <!--<font style="bold" />-->
    </fontcolor>
    
    <!-- less/sass keywords -->
    <fontcolor name="mod-private" foreColor="009090"> <!-- 00007C -->
        <font style="bold" />
    </fontcolor>
    
</fontscolors>
