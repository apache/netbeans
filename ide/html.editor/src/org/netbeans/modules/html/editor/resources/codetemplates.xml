    </codetemplate>
    <codetemplate abbreviation="in:radiol" xml:space="preserve" contexts="html-code">
        <code><![CDATA[<label>${text}
    <input type="radio" id="${id}" name="${name}" value="${value}">
</label>${cursor}]]></code>
    </codetemplate>
